<section class="brands">
    <h2>Nos marques</h2>
    <div class="brands__grid">
    {% for marque in marques %}    
        <div class="brand-card">
        <img src="/images/marques/{{ marque.imageName }}" alt="{{ marque.imageName }}">
        <p>{{ marque.name }}</p>
        </div>
    {% endfor %}
    </div>
 </section>